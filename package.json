    "jest": "^26.6.3",
    "jest-localstorage-mock": "^2.4.18",
    "lint-staged": "^10.5.4",
    "node-sass": "^4.14.1",
    "sass-loader": "^10.0.2",
    "style-loader": "^2.0.0",
