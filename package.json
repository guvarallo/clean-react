{
  "name": "clean-react",
  "version": "1.5.0",
  "description": "Survey App using React, TypeScript, Clean Architecture and TDD",
  "author": "Gustavo Varallo",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open",
    "test": "jest --passWithNoTests --no-cahe --runInBand",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info",
    "test:cypress": "cypress open",
    "check": "npm-check -u -s"
  },
  "keywords": [],
