    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.33",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "clean-webpack-plugin": "^3.0.0",
    "coveralls": "^3.1.1",
    "css-loader": "^5.1.2",
    "cypress": "^6.7.1",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
